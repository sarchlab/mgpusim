    "version": "0.2.0",
    "configurations": [{
            "name": "Stencil 2d",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/samples/stencil2d",
            "args": [
                "-verify"
            ]
        },
        {
            "name": "Matrix Transpose",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/samples/matrixtranspose",
            "args": [
                "-width=64"
            ]
        },
        {
            "name": "Bitonic Sort",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/samples/bitonicsort",
            "args": [
                "-verify",
                "-gpus=1,2"
            ]
        },
        {
            "name": "Test gitlab.com/akita/gcn3/driver",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/driver"
        },
        {
            "name": "Test gitlab.com/akita/gcn3/kernels",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/kernels"
        },
        {
            "name": "Test gitlab.com/akita/gcn3/timing",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/timing"
        },
        {
            "name": "Test gitlab.com/akita/gcn3/timing/newcache/l1v",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/timing/newcaches/l1v"
        },
        {
            "name": "L1V acceptance test",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/timing/newcaches/l1v/acceptancetest"
        },
        {
            "name": "FIR",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/samples/fir",
            "args": [
                "-verify",
                "-length=1024",
                "-gpus=1,2",
                "-debug-isa"
            ]
        },
        {
            "name": "Matrix Multiplication",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/samples/matrixmultiplication",
            "args": [
                "-timing",
                "-gpus=1,2",
                "-x=64",
                "-y=64",
                "-z=128"
            ]
        },
    ]
}